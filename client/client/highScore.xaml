<Window x:Class="client.highScore"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client"
        mc:Ignorable="d"
        Title="highScore" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="pictures/Leaderboard-blog.png"/>
    </Window.Background>
    <Grid>
        <Button x:Name="backButton" Content="back" HorizontalAlignment="Left" Margin="65,336,0,0" VerticalAlignment="Top" Click="Button_Click" Background="#FF050D11" Foreground="#FFFFFEFE"/>

        <TextBlock x:Name="textTitle" HorizontalAlignment="Left" Margin="42,10,0,0" TextWrapping="Wrap" Text="Leader Board" VerticalAlignment="Top" FontSize="30" FontFamily="Segoe UI Black" Height="38" Width="222"/>
        <TextBlock x:Name="heighScore" HorizontalAlignment="Left" Margin="42,78,0,0" TextWrapping="Wrap" Text="none" VerticalAlignment="Top" FontFamily="Segoe UI Black" FontSize="25"/>

    </Grid>
</Window>
